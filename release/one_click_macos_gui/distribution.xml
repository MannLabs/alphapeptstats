<?xml version="1.0" encoding="utf-8" standalone="no"?>
<installer-script minSpecVersion="1.000000">
    <title>AlphaPeptStats 0.3.0</title>
    <background mime-type="image/png" file="alphapeptstats_logo.png" scaling="proportional"/>
    <welcome file="welcome.html" mime-type="text/html" />
    <conclusion file="conclusion.html" mime-type="text/html" />
    <license file="LICENSE.txt"/>
    <options customize="never" allow-external-scripts="no"/>
    <domains enable_localSystem="true" />
    <choices-outline>
        <line choice="AlphaPeptStats"/>
    </choices-outline>
    <choice id="AlphaPeptStats" title="AlphaPeptStats">
        <pkg-ref id="AlphaPeptStats.pkg"/>
    </choice>
    <pkg-ref id="AlphaPeptStats.pkg" auth="Root">AlphaPeptStats.pkg</pkg-ref>
</installer-script>
